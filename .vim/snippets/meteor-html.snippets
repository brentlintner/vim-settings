snippet tmpl
	<template name="${1}">
		${2}
	</template>
snippet with
	{{#with ${1}}}
		${2}
	{{/with}}
snippet each
	{{#each ${1}}}
		${2}	
	{{/each}}
snippet link
	{{#linkTo "${1}"}}
		${2}
	{{/linkTo}}
snippet group
	<!---------------------------------------------------------------------------->
	<!-- ${1} -->
	<!---------------------------------------------------------------------------->
snippet incl
	{{> ${1}}}
snippet block
	{{#${1}}}
		${2}
	{{/$1}}
